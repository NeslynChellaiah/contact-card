lass Solution:
    def flatten(self, head: 'Node') -> 'Node':
        cur = head
        s = []
        while head:
            if head.child:
                if head.next:
                    s.append(head.next)
                head.next = head.child
                head.child.prev = head
                head.child = None
            if(not head.next and s):
                head.next = s.pop()
                head.next.prev = head
            head = head.next
        return cur
